#: ../data/mousetweaks.ui.h:1
msgid "Hover Click"
msgstr "لەيلىتىپ چەك"

#: ../data/mousetweaks.ui.h:2
msgid "Single Click"
msgstr "تاق چەك"

#: ../data/mousetweaks.ui.h:3
msgid "Double Click"
msgstr "قوش چېكىش"

#. 'Drag' like in a Drag and Drop operation
#: ../data/mousetweaks.ui.h:5
msgid "Drag"
msgstr "سۆرەش"

#: ../data/mousetweaks.ui.h:6
msgid "Secondary Click"
msgstr "ئىككىنچى چېكىش"

#: ../data/mousetweaks.ui.h:7
msgid "Button Style"
msgstr "توپچا ئۇسلۇبى"

#: ../data/mousetweaks.ui.h:8
msgid "Text only"
msgstr "تېكىستلا"

#: ../data/mousetweaks.ui.h:9
msgid "Icons only"
msgstr "سىنبەلگىلەرلا"

#: ../data/mousetweaks.ui.h:10
msgid "Text and Icons"
msgstr "تېكىست ۋە سىنبەلگىلەر"

#: ../data/mousetweaks.ui.h:11
msgid "Orientation"
msgstr "يۆنىلىش"

#. Buttons are arranged from left to right in the layout
#: ../data/mousetweaks.ui.h:13
msgid "Horizontal"
msgstr "توغرا"

#. Buttons are arranged from top to bottom in the layout
#: ../data/mousetweaks.ui.h:15
msgid "Vertical"
msgstr "تىك"

#: ../data/org.gnome.mousetweaks.gschema.xml.in.h:1
msgid "Click-type window style"
msgstr "چېكىش تىپىدىكى كۆزنەكنىڭ ئۇسلۇبى"

#: ../data/org.gnome.mousetweaks.gschema.xml.in.h:2
msgid "Button style of the click-type window."
msgstr "چېكىش تىپىدىكى كۆزنەكتىكى توپچىنىڭ ئۇسلۇبى"

#: ../data/org.gnome.mousetweaks.gschema.xml.in.h:3
msgid "Click-type window orientation"
msgstr "چېكىش تىپىدىكى كۆزنەكنىڭ يۆنىلىشى"

#: ../data/org.gnome.mousetweaks.gschema.xml.in.h:4
msgid "Orientation of the click-type window."
msgstr "چېكىش تىپىدىكى كۆزنەكنىڭ يۆنىلىشى."

#: ../data/org.gnome.mousetweaks.gschema.xml.in.h:5 ../src/mt-main.c:638
msgid "Click-type window geometry"
msgstr "چېكىش تىپىدىكى كۆزنەكنىڭ شەكلى"

#: ../data/org.gnome.mousetweaks.gschema.xml.in.h:6
msgid ""
"Size and position of the click-type window. The format is a standard X "
"Window System geometry string."
msgstr "چېكىش تىپىدىكى كۆزنەكنىڭ چوڭلۇقى ۋە ئورنى. ئۇنىڭ پىچىمى X كۆزنەك سىستېمىسىدىكى شەكىل تىزىقى ئۆلچىمى بىلەن ئوخشاش."

#: ../src/mt-main.c:624
msgid "Enable dwell click"
msgstr "توختىتىپ چېكىشنى قوزغات"

#: ../src/mt-main.c:626
msgid "Enable simulated secondary click"
msgstr "تەقلىدلىك ئىككىنچى چېكىشنى قوزغات"

#: ../src/mt-main.c:628
msgid "Time to wait before a dwell click"
msgstr "توختىتىپ چېكىش ئالدىدىكى كۈتۈش ۋاقتى"

#: ../src/mt-main.c:630
msgid "Time to wait before a simulated secondary click"
msgstr "تەقلىدلىك ئىككىنچى چېكىش ئالدىدىكى كۈتۈش ۋاقتى"

#: ../src/mt-main.c:632
msgid "Set the active dwell mode"
msgstr "ئاكتىپ توختىتىش(dwell) ھالىتىنى بەلگىلەش"

#: ../src/mt-main.c:634
#| msgid "Orientation of the click-type window."
msgid "Hide the click-type window"
msgstr "چېكىش تىپىدىكى كۆزنەكنى يوشۇر"

#: ../src/mt-main.c:636
msgid "Ignore small pointer movements"
msgstr "ئىسترېلكىنىڭ كىچىك يۆتكىلىشىگە پەرۋا قىلما"

#: ../src/mt-main.c:640
msgid "Shut down mousetweaks"
msgstr "mousetweaks نى تاقا"

#: ../src/mt-main.c:642
msgid "Start mousetweaks as a daemon"
msgstr "mousetweaks نى مۇئەككەل شەكلىدە باشلا"

#: ../src/mt-main.c:644
msgid "Start mousetweaks in login mode"
msgstr "mousetweaks نى كىرىش(login) شەكلىدە باشلا"

#. parse
#: ../src/mt-main.c:662
msgid "- GNOME mouse accessibility daemon"
msgstr "- گىنوم چاشقىنەكنىڭ ياردەمچى ئىقتىدارى مۇئەككىلى"

